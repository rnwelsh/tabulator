{
  "name": "tabulator-tables",
  "version": "5.4.2",
  "description": "Interactive table generation JavaScript library",
  "style": "dist/css/tabulator.css",
  "main": "dist/js/tabulator.js",
  "module": "dist/js/tabulator_esm.js",
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "packageManager": "^pnpm@7.17.1",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w --environment TARGET:dev",
    "dev:css": "rollup -c -w --environment TARGET:css",
    "dev:esm": "rollup -c -w --environment TARGET:esm",
    "dev:umd": "rollup -c -w --environment TARGET:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rnwelsh/tabulator.git"
  },
  "author": "Oli Folkerd",
  "license": "MIT",
  "homepage": "http://tabulator.info/",
  "devDependencies": {
    "esbuild": "^0.15.16",
    "fs-extra": "^9.1.0",
    "globby": "^11.1.0",
    "postcss": "^8.4.19",
    "postcss-prettify": "^0.3.4",
    "rollup": "^3.5.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sizes": "^1.0.4",
    "sass": "^1.56.1"
  }
}
